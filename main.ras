        end;
        
        if(Key::Pressed(key_down) or Key::Pressed(n_key_down)) then
        begin
            if(y<24) then
            begin
                inc(y);
                if y> 15 then sy:=sy+1;
            end;
        end;
        if(Key::Pressed(key_left) or Key::Pressed(n_key_left)) then
        begin
            if(x>0) then
            begin
                dec(x);
            end;
        end;
        
        
        //draw_frame();
		
	end;

end.



